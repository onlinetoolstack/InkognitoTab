{
    "manifest_version": 3,
    "name": "InkognitoTab",
    "author": "OnlineToolStack.com Developers",
    "version": "1.1",
    "description": "Easily switch regular browsing between and incognito modes with the click of a button using InkognitoTab extension",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "16": "32.png",
            "24": "32.png",
            "32": "32.png",
            "64": "64.png",
            "128": "128.png"
        }
    },
    "icons": {
        "16": "32.png",
        "24": "32.png",
        "32": "32.png",
        "64": "64.png",
        "128": "128.png"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Q",
                "windows": "Ctrl+Q",
                "mac": "Command+Q",
                "chromeos": "Ctrl+Q",
                "linux": "Ctrl+Q"
            }
        }
    },
    "host_permissions": [
        "<all_urls>"
    ]
}